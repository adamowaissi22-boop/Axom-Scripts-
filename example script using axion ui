local Axion = loadstring(game:HttpGet("https://raw.githubusercontent.com/adamowaissi22-boop/Axom-Scripts-/refs/heads/main/Axion%20Ui%20Library"))()

local Window = Axion:CreateWindow({
    Name = "Axion - Demo Script",
    Subtitle = "UI Library Demo",
    Version = "v1.0",
    LoadingTitle = "Demo Script",
    LoadingSubtitle = "Axion Demo",
    Theme = "Default",
    ConfigurationSaving = {
        Enabled = true,
        FolderName = "AxionConfig",
        FileName = "DemoConfig"
    },
    AnimationSpeed = 0.3,
    RippleEnabled = true,
    CornerRadius = 12,
    ToggleKey = Enum.KeyCode.RightShift
})

local ButtonTab = Window:CreateTab({
    Name = "Button",
    Icon = "üü¶"
})

local ToggleTab = Window:CreateTab({
    Name = "Toggle",
    Icon = "üîò"
})

local SliderTab = Window:CreateTab({
    Name = "Slider",
    Icon = "üìä"
})

local DropdownTab = Window:CreateTab({
    Name = "Dropdown",
    Icon = "üìã"
})

local InputTab = Window:CreateTab({
    Name = "Input",
    Icon = "‚å®Ô∏è"
})

local KeybindTab = Window:CreateTab({
    Name = "Keybind",
    Icon = "üîë"
})

local ColorPickerTab = Window:CreateTab({
    Name = "Color Picker",
    Icon = "üé®"
})

local NotificationTab = Window:CreateTab({
    Name = "Notification",
    Icon = "üîî"
})

local SettingsTab = Window:CreateTab({
    Name = "Settings",
    Icon = "‚öôÔ∏è"
})

Window:Notify({
    Title = "Welcome",
    Content = "Axion Demo Script Loaded Successfully!",
    Duration = 5,
    Type = "Success"
})

local ButtonSection = ButtonTab:CreateSection("Button Examples")

local button1 = ButtonSection:CreateButton({
    Name = "Show Info Notification",
    Callback = function()
        Window:Notify({
            Title = "Info Notification",
            Content = "This is an info notification from Button 1",
            Duration = 3,
            Type = "Info"
        })
    end
})

local button2 = ButtonSection:CreateButton({
    Name = "Teleport to Spawn",
    Callback = function()
        local player = game.Players.LocalPlayer
        if player.Character then
            player.Character.HumanoidRootPart.CFrame = CFrame.new(0, 5, 0)
            Window:Notify({
                Title = "Teleported",
                Content = "Teleported to spawn point!",
                Duration = 3,
                Type = "Success"
            })
        end
    end
})

local button3 = ButtonSection:CreateButton({
    Name = "Kill Character",
    Callback = function()
        Window:Dialog({
            Title = "Confirm Kill",
            Content = "Are you sure you want to kill your character?",
            Buttons = {
                {
                    Title = "Yes",
                    Primary = true,
                    Callback = function()
                        game.Players.LocalPlayer.Character.Humanoid.Health = 0
                        Window:Notify({
                            Title = "Character Killed",
                            Content = "Your character has been killed!",
                            Duration = 3,
                            Type = "Warning"
                        })
                    end
                },
                {
                    Title = "No",
                    Callback = function()
                        Window:Notify({
                            Title = "Cancelled",
                            Content = "Kill action cancelled",
                            Duration = 2,
                            Type = "Info"
                        })
                    end
                }
            }
        })
    end
})

local button4 = ButtonSection:CreateButton({
    Name = "Test Dialog",
    Callback = function()
        Window:Dialog({
            Title = "Test Dialog",
            Content = "This is a test dialog box with multiple buttons",
            Buttons = {
                {
                    Title = "OK",
                    Primary = true,
                    Callback = function()
                        Window:Notify({
