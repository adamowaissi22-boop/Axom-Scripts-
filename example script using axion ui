    Name = "Multiple Selection",
    Options = {"Option 1", "Option 2", "Option 3", "Option 4", "Option 5"},
    MultiSelect = true,
    Flag = "MultiSelect",
    Callback = function(values)
        Window:Notify({
            Title = "Multiple Selection",
            Content = "Selected: " .. table.concat(values, ", "),
            Duration = 3,
            Type = "Info"
        })
    end
})

local InputSection = InputTab:CreateSection("Input Examples")

local input1 = InputSection:CreateInput({
    Name = "Teleport to Player",
    PlaceholderText = "Enter username...",
    RemoveTextAfterFocusLost = true,
    Flag = "TeleportTarget",
    Callback = function(value)
        local found = false
        for _, player in pairs(game.Players:GetPlayers()) do
            if player.Name:lower():find(value:lower()) or player.DisplayName:lower():find(value:lower()) then
                if player.Character then
                    game.Players.LocalPlayer.Character:SetPrimaryPartCFrame(player.Character.HumanoidRootPart.CFrame)
                    Window:Notify({
                        Title = "Teleported",
                        Content = "Teleported to " .. player.DisplayName,
                        Duration = 3,
                        Type = "Success"
                    })
                    found = true
                end
                break
            end
        end
        if not found then
            Window:Notify({
                Title = "Error",
                Content = "Player not found!",
                Duration = 3,
                Type = "Error"
            })
        end
    end
})

local input2 = InputSection:CreateInput({
    Name = "Set Cash Amount",
    PlaceholderText = "Enter amount...",
    Flag = "CashAmount",
    Callback = function(value)
        local amount = tonumber(value)
        if amount then
            Window:Notify({
                Title = "Cash Set",
                Content = "Set cash to " .. amount,
                Duration = 3,
                Type = "Success"
            })
        else
            Window:Notify({
                Title = "Error",
                Content = "Invalid number!",
                Duration = 3,
                Type = "Error"
            })
        end
    end
})

local KeybindSection = KeybindTab:CreateSection("Keybind Examples")

local keybind1 = KeybindSection:CreateKeybind({
    Name = "Toggle UI",
    CurrentKeybind = "RightShift",
    Flag = "ToggleUIKey",
    Callback = function()
        Window:Toggle()
        Window:Notify({
            Title = "UI Toggled",
            Content = "Interface visibility changed",
            Duration = 2,
            Type = "Info"
        })
    end
})

local keybind2 = KeybindSection:CreateKeybind({
    Name = "NoClip Toggle",
    CurrentKeybind = "N",
    Flag = "NoClipKey",
    Callback = function()
        toggle1:Set(not toggle1.Value)
    end
})

local keybind3 = KeybindSection:CreateKeybind({
    Name = "Fly (Hold)",
    CurrentKeybind = "F",
    Flag = "FlyKey",
    HoldToInteract = true,
    Callback = function(value)
        if value then
            Window:Notify({
                Title = "Fly",
                Content = "Fly activated",
                Duration = 2,
                Type = "Success"
            })
        else
            Window:Notify({
                Title = "Fly",
                Content = "Fly deactivated",
                Duration = 2,
                Type = "Warning"
            })
        end
    end
})

local ColorPickerSection = ColorPickerTab:CreateSection("Color Picker Examples")

local colorpicker1 = ColorPickerSection:CreateColorPicker({
    Name = "ESP Color",
    Color = Color3.fromRGB(255, 0, 0),
    Flag = "ESPColor",
    Callback = function(color)
        for _, player in pairs(game.Players:GetPlayers()) do
            if player ~= game.Players.LocalPlayer then
                local esp = player.Character:FindFirstChild("AxionESP")
                if esp then
                    esp.FillColor = color
                end
            end
        end
        Window:Notify({
            Title = "ESP Color",
            Content = "Color updated to RGB(" .. math.floor(color.R * 255) .. ", " .. math.floor(color.G * 255) .. ", " .. math.floor(color.B * 255) .. ")",
            Duration = 3,
            Type = "Info"
        })
    end
})

local colorpicker2 = ColorPickerSection:CreateColorPicker({
    Name = "Interface Color",
    Color = Color3.fromRGB(100, 120, 255),
    Flag = "InterfaceColor",
    Callback = function(color)
        Window:Notify({
            Title = "Interface Color",
            Content = "Color changed",
            Duration = 2,
            Type = "Info"
        })
    end
})

