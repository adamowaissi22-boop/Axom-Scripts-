local SettingsSection = SettingsTab:CreateSection("Settings")

local themeDropdown = SettingsSection:CreateDropdown({
    Name = "Select Theme",
    Options = {"Default", "Dark", "Light", "Ocean", "Midnight", "Emerald", "Crimson", "Galaxy", "Sunset", "Cyberpunk"},
    CurrentOption = "Default",
    Flag = "Theme",
    Callback = function(value)
        Window:SetTheme(value)
        Window:Notify({
            Title = "Theme Changed",
            Content = "Changed to " .. value .. " theme",
            Duration = 3,
            Type = "Success"
        })
    end
})

local resetButton = SettingsSection:CreateButton({
    Name = "Reset All Settings",
    Callback = function()
        Window:Dialog({
            Title = "Reset Settings",
            Content = "Are you sure you want to reset ALL settings to default?",
            Buttons = {
                {
                    Title = "Yes",
                    Primary = true,
                    Callback = function()
                        toggle1:Set(false)
                        toggle2:Set(false)
                        toggle3:Set(false)
                        toggle4:Set(false)
                        slider1:Set(16)
                        slider2:Set(50)
                        slider3:Set(70)
                        slider4:Set(196)
                        dropdown1:Set("Sword")
                        dropdown2:Set("Red")
                        dropdown3:Set({})
                        colorpicker1:Set(Color3.fromRGB(255, 0, 0))
                        colorpicker2:Set(Color3.fromRGB(100, 120, 255))
                        themeDropdown:Set("Default")
                        
                        Window:Notify({
                            Title = "Settings Reset",
                            Content = "All settings have been reset to default!",
                            Duration = 4,
                            Type = "Success"
                        })
                    end
                },
                {
                    Title = "No",
                    Callback = function()
                        Window:Notify({
                            Title = "Cancelled",
                            Content = "Reset cancelled",
                            Duration = 2,
                            Type = "Info"
                        })
                    end
                }
            }
        })
    end
})

local destroyButton = SettingsSection:CreateButton({
    Name = "Destroy UI",
    Callback = function()
        Window:Dialog({
            Title = "Destroy UI",
            Content = "Are you sure you want to destroy the interface?",
            Buttons = {
                {
                    Title = "Yes",
                    Primary = true,
                    Callback = function()
                        Window:Destroy()
                    end
                },
                {
                    Title = "No",
                    Callback = function()
                        Window:Notify({
                            Title = "Cancelled",
                            Content = "UI destruction cancelled",
                            Duration = 2,
                            Type = "Info"
                        })
                    end
                }
            }
        })
    end
})

local InfoSection = SettingsTab:CreateSection("Information")

