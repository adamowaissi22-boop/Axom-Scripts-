local AllPlayersTab = Window:CreateTab({Name = "All Players", Icon = "üë•"})
local AllPlayersSection = AllPlayersTab:CreateSection({Name = "All Players Commands"})

local CharAllToggle = AllPlayersSection:CreateToggle({
    Name = "Char All",
    Flag = "CharAllToggle",
    CurrentValue = false,
    Callback = function(value)
        if value then
            RequestCommand:InvokeServer(";char all adamofficial0022")
