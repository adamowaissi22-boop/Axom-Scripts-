local selectedPlayer = nil
local targetBox = TrollTab:CreateBox({
    Text = "Target Player",
    Placeholder = "Enter username",
    Default = "",
    Callback = function(value)
        if value and value ~= "" then
            local player = GetPlayerByName(value)
            if player then
                selectedPlayer = player
            else
                selectedPlayer = nil
            end
        else
            selectedPlayer = nil
        end
    end
})

TrollTab:CreateButton({
    Text = "Teleport To Player",
    Callback = function()
        if selectedPlayer then
            TeleportToPlayer(selectedPlayer)
        end
    end
})

TrollTab:CreateToggle({
    Text = "Watch Player",
    Default = false,
    Callback = function(enabled)
        Watching = enabled
        if enabled and selectedPlayer then
            StartWatching(selectedPlayer)
        else
            StopWatching()
        end
    end
})

TrollTab:CreateToggle({
    Text = "Head Sit",
    Default = false,
    Callback = function(enabled)
        HeadSit = enabled
        if enabled and selectedPlayer then
            StartHeadSit(selectedPlayer)
        else
            StopHeadSit()
        end
    end
})

TrollTab:CreateToggle({
    Text = "Backpack",
    Default = false,
    Callback = function(enabled)
        Backpack = enabled
        if enabled and selectedPlayer then
            StartBackpack(selectedPlayer)
        else
            StopBackpack()
        end
    end
})

TrollTab:CreateToggle({
    Text = "WalkTo/Follow",
    Default = false,
    Callback = function(enabled)
        FollowPlayer = enabled
        if enabled and selectedPlayer then
            StartFollowPlayer(selectedPlayer)
        else
            StopFollowPlayer()
        end
    end
})

TrollTab:CreateToggle({
    Text = "Fling",
    Default = false,
    Callback = function(enabled)
        if enabled and selectedPlayer then
            ToggleFling(true)
        else
            ToggleFling(false)
        end
    end
})

TrollTab:CreateButton({
    Text = "Click Target Tool",
    Callback = function()
        local GetTargetTool = Instance.new("Tool")
        GetTargetTool.Name = "ClickTarget"
        GetTargetTool.RequiresHandle = false
        GetTargetTool.TextureId = "rbxassetid://2716591855"
        GetTargetTool.ToolTip = "Select Target"

        local function ActivateTool()
            local mouse = LocalPlayer:GetMouse()
            local hit = mouse.Target
            local person = nil
            if hit and hit.Parent then
                if hit.Parent:IsA("Model") then
                    person = game.Players:GetPlayerFromCharacter(hit.Parent)
                elseif hit.Parent:IsA("Accessory") then
                    person = game.Players:GetPlayerFromCharacter(hit.Parent.Parent)
                end
                if person then
                    selectedPlayer = person
                    -- Update the target box with the selected player's name
                    targetBox:Set(person.Name)
                end
            end
        end

        GetTargetTool.Activated:Connect(function()
            ActivateTool()
        end)
        GetTargetTool.Parent = LocalPlayer.Backpack
    end
})

