local Apoto = loadstring(game:HttpGet("https://raw.githubusercontent.com/adamowaissi22-boop/Axom-Scripts-/refs/heads/main/Apoto%20Library%20Gui"))()

local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local TweenService = game:GetService("TweenService")
local LocalPlayer = Players.LocalPlayer

local Checkpoint
local NoclipLoop
local EspObjects = {}
local XrayParts = {}
local Watching = false
local HeadSit = false
local Backpack = false
local FollowPlayer = false
local AntiLagEnabled = false
local HighQualityEnabled = false
local FlingEnabled = false

local window = Apoto:CreateWindow({
    Name = "Axorcy : the main script",
    Icon = "rbxassetid://18820555586",
    AccentColor3 = Color3.new(0.2, 0.6, 1)
})

local PlayerTab = window:CreateTab({
    Name = "Player",
    Icon = "rbxassetid://7733800044"
})

local ToolsTab = window:CreateTab({
    Name = "Tools", 
    Icon = "rbxassetid://7733800044"
})

local TrollTab = window:CreateTab({
    Name = "Troll",
    Icon = "rbxassetid://7733800044"
})

local MiscTab = window:CreateTab({
    Name = "Misc",
    Icon = "rbxassetid://7733800044"
})

local AnimationsTab = window:CreateTab({
    Name = "Animations",
    Icon = "rbxassetid://7733800044"
})

function GetRoot(player)
    return player.Character and player.Character:FindFirstChild("HumanoidRootPart")
end

function StartNoclip()
    NoclipLoop = RunService.Stepped:Connect(function()
        if LocalPlayer.Character then
            for _, part in pairs(LocalPlayer.Character:GetDescendants()) do
                if part:IsA("BasePart") then
                    part.CanCollide = false
                end
            end
        end
    end)
end

function StopNoclip()
    if NoclipLoop then
        NoclipLoop:Disconnect()
    end
end

function StartESP()
    for _, player in pairs(Players:GetPlayers()) do
        if player ~= LocalPlayer and player.Character then
            CreateESP(player.Character)
        end
    end
    
    Players.PlayerAdded:Connect(function(player)
        player.CharacterAdded:Connect(function(character)
            CreateESP(character)
        end)
    end)
end

function CreateESP(character)
    local highlight = Instance.new("Highlight")
    highlight.Adornee = character
    highlight.FillColor = Color3.fromRGB(255, 0, 0)
    highlight.OutlineColor = Color3.fromRGB(255, 255, 255)
    highlight.Parent = character
    EspObjects[character] = highlight
end

function StopESP()
    for character, highlight in pairs(EspObjects) do
        highlight:Destroy()
    end
    EspObjects = {}
end

function StartXray()
    for _, part in pairs(workspace:GetDescendants()) do
        if part:IsA("BasePart") and part.Transparency < 1 then
            XrayParts[part] = part.Transparency
            part.Transparency = 0.5
        end
    end
end

function StopXray()
    for part, transparency in pairs(XrayParts) do
        part.Transparency = transparency
    end
    XrayParts = {}
end

function SaveCheckpoint()
    if LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
        Checkpoint = LocalPlayer.Character.HumanoidRootPart.CFrame
    end
end

function RemoveCheckpoint()
    Checkpoint = nil
end

function TeleportToCheckpoint()
    if Checkpoint and LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
        LocalPlayer.Character.HumanoidRootPart.CFrame = Checkpoint
    end
end

function TeleportToPlayer(player)
    if player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
        LocalPlayer.Character.HumanoidRootPart.CFrame = player.Character.HumanoidRootPart.CFrame
    end
end

function StartWatching(player)
    if player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
        workspace.CurrentCamera.CameraSubject = player.Character.HumanoidRootPart
    end
end

function StopWatching()
    workspace.CurrentCamera.CameraSubject = LocalPlayer.Character:FindFirstChildOfClass("Humanoid")
end

function StartHeadSit(player)
    coroutine.wrap(function()
        while HeadSit and player.Character do
            if LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("Humanoid") then
                pcall(function()
                    if not GetRoot(LocalPlayer):FindFirstChild("BreakVelocity") then
                        local Velocity = Instance.new("BodyVelocity")
                        Velocity.Name = "BreakVelocity"
                        Velocity.Parent = GetRoot(LocalPlayer)
                    end
                    
                    local targethead = player.Character.Head
                    LocalPlayer.Character.Humanoid.Sit = true
                    GetRoot(LocalPlayer).CFrame = targethead.CFrame * CFrame.new(0, 2, 0)
                    GetRoot(LocalPlayer).Velocity = Vector3.new(0, 0, 0)
                end)
            end
            wait()
        end
        
        if GetRoot(LocalPlayer) and GetRoot(LocalPlayer):FindFirstChild("BreakVelocity") then
            GetRoot(LocalPlayer).BreakVelocity:Destroy()
        end
    end)()
end

function StopHeadSit()
    if GetRoot(LocalPlayer) and GetRoot(LocalPlayer):FindFirstChild("BreakVelocity") then
        GetRoot(LocalPlayer).BreakVelocity:Destroy()
    end
end

function StartBackpack(player)
    coroutine.wrap(function()
        while Backpack and player.Character do
            if LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("Humanoid") then
                pcall(function()
                    if not GetRoot(LocalPlayer):FindFirstChild("BreakVelocity") then
                        local Velocity = Instance.new("BodyVelocity")
                        Velocity.Name = "BreakVelocity"
                        Velocity.Parent = GetRoot(LocalPlayer)
                    end
                    
                    local root = GetRoot(player)
                    LocalPlayer.Character.Humanoid.Sit = true
                    GetRoot(LocalPlayer).CFrame = root.CFrame * CFrame.new(0, 0, 1.2) * CFrame.Angles(0, -3, 0)
                    GetRoot(LocalPlayer).Velocity = Vector3.new(0, 0, 0)
                end)
            end
            wait()
        end
        
        if GetRoot(LocalPlayer) and GetRoot(LocalPlayer):FindFirstChild("BreakVelocity") then
            GetRoot(LocalPlayer).BreakVelocity:Destroy()
        end
    end)()
end

function StopBackpack()
    if GetRoot(LocalPlayer) and GetRoot(LocalPlayer):FindFirstChild("BreakVelocity") then
        GetRoot(LocalPlayer).BreakVelocity:Destroy()
    end
end

function StartFollowPlayer(player)
    coroutine.wrap(function()
        while FollowPlayer and player.Character do
            if LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("Humanoid") then
                local humanoid = LocalPlayer.Character.Humanoid
                local targetPos = player.Character.HumanoidRootPart.Position
                local currentPos = LocalPlayer.Character.HumanoidRootPart.Position
                local distance = (targetPos - currentPos).Magnitude
                
                if distance > 5 then
                    humanoid:MoveTo(targetPos)
                else
                    humanoid:MoveTo(currentPos)
                end
            end
            wait(0.1)
        end
    end)()
end

function StopFollowPlayer()
    if LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("Humanoid") then
        LocalPlayer.Character.Humanoid:MoveTo(LocalPlayer.Character.HumanoidRootPart.Position)
    end
end

function ToggleFling(enabled)
    FlingEnabled = enabled
    if enabled then
        if LocalPlayer.Character and GetRoot(LocalPlayer) then
            local BodyVelocity = Instance.new("BodyVelocity")
            BodyVelocity.Velocity = Vector3.new(0, 1000, 0)
            BodyVelocity.MaxForce = Vector3.new(1, 1, 1) * 10000
            BodyVelocity.Parent = GetRoot(LocalPlayer)
        end
    else
        if LocalPlayer.Character and GetRoot(LocalPlayer) then
            for _, part in pairs(GetRoot(LocalPlayer):GetChildren()) do
                if part:IsA("BodyVelocity") then
                    part:Destroy()
                end
            end
        end
    end
end

function EnableAntiLag()
    for _, lighting in pairs(game:GetService("Lighting"):GetChildren()) do
        if lighting:IsA("PostEffect") then
            lighting.Enabled = false
        end
    end
    settings().Rendering.QualityLevel = 1
end

function DisableAntiLag()
    for _, lighting in pairs(game:GetService("Lighting"):GetChildren()) do
        if lighting:IsA("PostEffect") then
            lighting.Enabled = true
        end
    end
    settings().Rendering.QualityLevel = 10
end

function EnableHighQuality()
    game:GetService("Lighting").GlobalShadows = true
    game:GetService("Lighting").Technology = "Voxel"
    settings().Rendering.QualityLevel = 21
end

function DisableHighQuality()
    game:GetService("Lighting").GlobalShadows = false
    game:GetService("Lighting").Technology = "Legacy"
    settings().Rendering.QualityLevel = 10
end

function StopAnim()
    if LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("Humanoid") then
        LocalPlayer.Character.Humanoid:ChangeState(Enum.HumanoidStateType.Physics)
    end
end

function GetPlayerByName(name)
    for _, player in pairs(Players:GetPlayers()) do
        if string.lower(player.Name) == string.lower(name) or string.lower(player.DisplayName) == string.lower(name) then
            return player
        end
    end
    return nil
end

