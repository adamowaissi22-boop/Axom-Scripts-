local walkSpeedBox = PlayerTab:CreateBox({
    Text = "WalkSpeed",
    Placeholder = "Enter speed value",
    Default = "16",
    Callback = function(value)
        local speed = tonumber(value)
        if speed and LocalPlayer.Character then
            LocalPlayer.Character:FindFirstChildOfClass("Humanoid").WalkSpeed = speed
        end
    end
})

local jumpPowerBox = PlayerTab:CreateBox({
    Text = "Jump Power", 
    Placeholder = "Enter jump power value",
    Default = "50",
    Callback = function(value)
        local power = tonumber(value)
        if power and LocalPlayer.Character then
            LocalPlayer.Character:FindFirstChildOfClass("Humanoid").JumpPower = power
        end
    end
})

